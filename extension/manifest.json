{
  "name": "flylinks",
  "description": "flylinks: Chrome extensions for interacting with self-hosted golinks",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png",
    "512": "icon.png"
  },
  "permissions": [
    "activeTab",
    "*://go/*",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ]
}
