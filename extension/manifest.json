{
  "name": "flylinks",
  "description": "flylinks: Chrome extensions for interacting with self-hosted golinks",
  "version": "1.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png",
    "512": "icon.png"
  },
  "permissions": [
    "activeTab",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess",
    "nativeMessaging",
    "storage"
  ],
  "host_permissions": ["<all_urls>"]
}
